<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<OutputType>Library</OutputType>
		<!-- Ensures it's a class library -->
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<!-- Automatically generates a NuGet package -->
		<PackageId>StringExtensions</PackageId>
		<!-- Change to your desired package name -->
		<Version>1.0.0</Version>
		<Authors>YourName</Authors>
		<Description>Checks if a string starts with an uppercase letter.</Description>
	</PropertyGroup>

</Project>
